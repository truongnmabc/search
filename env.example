# Server Configuration
PORT=3000
NODE_ENV=development

# Elasticsearch Configuration
ELASTICSEARCH_URL=http://localhost:9200
ELASTICSEARCH_INDEX=search_documents

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PREFIX=search:

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/search_db

# ML Model Configuration
BERT_MODEL_PATH=./models/bert-base-uncased
VECTOR_DIMENSION=768

# Search Configuration
MAX_RESULTS_LAYER1=10000
MAX_RESULTS_LAYER2=1000
MAX_RESULTS_LAYER3=100
MAX_FINAL_RESULTS=20

# Personalization Configuration
USER_PROFILE_WEIGHT=0.3
CONTEXT_WEIGHT=0.2
TEMPORAL_WEIGHT=0.1
